{
  "name": "nadesiko3electron",
  "productName": "nadesiko3",
  "version": "3.4.27",
  "description": "Nadesiko3 packager for electron",
  "main": "start.cjs",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "build:prepare": "cp -r node_modules/nadesiko3/release/* ./webapp/nadesiko3/",
    "build:win": "electron-packager . --icon=res/nako.ico --platform=win32 --arch=x64,ia32 --overwrite",
    "build:mac": "electron-packager . --icon=res/AppIcon.icns --platform=darwin --arch=x64,arm64 --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kujirahand/nadesiko3electron.git"
  },
  "keywords": [],
  "author": "kujirahand",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kujirahand/nadesiko3electron/issues"
  },
  "homepage": "https://github.com/kujirahand/nadesiko3electron#readme",
  "devDependencies": {
    "electron": "^28.1.0",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "iconv-lite": "^0.6.3",
    "node-fetch": "^3.3.2",
    "opener": "^1.5.2",
    "shell-quote": "^1.8.0",
    "nadesiko3": "3.4.27"
  }
}
